<?xml version="1.0" encoding="utf-8"?>
<bindings xmlns="http://java.sun.com/xml/ns/jaxb" 
		xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
		xmlns:xsd="http://www.w3.org/2001/XMLSchema"
		xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" version="2.0">
	<globalBindings generateIsSetMethod="true">
		 <xjc:simple />
	</globalBindings>
	<bindings schemaLocation="../../resources/schemas/content.xsd" 
			node="/xsd:schema">
		<schemaBindings>
			<package name="org.migus.web.content.types"/>
		</schemaBindings>
	</bindings>
	<bindings schemaLocation="../../resources/schemas/content-service.xsd"
			node="/xsd:schema">
		<schemaBindings>
			<package name="org.migus.web.content.soap"/>
		</schemaBindings>
	</bindings>
	<bindings schemaLocation="../../resources/schemas/content-fault.xsd"
			node="/xsd:schema">
		<schemaBindings>
			<package name="org.migus.web.content.soap"/>
		</schemaBindings>
	</bindings>
	<bindings schemaLocation="../../resources/wsdl/content-service.wsdl"
			node="/xsd:schema[@targetNamespace='http://migus.org/webservices/content-service']"/>
</bindings>
